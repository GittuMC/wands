{
  "schema_version": 1,
  "mixin": [
    "wands.mixins.json",
    "wands-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "wands",
    "version": "${version}",
    "name": "Buildin Wands",
    "description": "This is an example description! Tell everyone what your mod is about!",
    "authors": ["Me!"],
    "contact": {
      "sources": "https://github.com/nicguzzo/wands"
    },
    "license": "Insert License Here",
    "icon": "assets/wands/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "net.nicguzzo.wands.quilt.WandsModQuilt"
      ],
      "client_init": "net.nicguzzo.wands.quilt.WandsModClientQuilt"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19"
      },
      {
        "id": "architectury",
        "version": ">=5.7.28"
      }
    ]
  }
}